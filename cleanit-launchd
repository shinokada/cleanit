#!/usr/bin/env bash

# path to notifier
notifier=changenotifierpath
# path to cleanit
cleanit=changecleanitpath
# clean-up Download
# store the output to a variable
cleanup_notice=$($cleanit)
# clean-up Trash
# trash_notice=$($cleanit trash -D changedays)

$notifier -title Cleanit -subtitle "Daily Clean-up" -message "Downloads cleanup: ${cleanup_notice}"
